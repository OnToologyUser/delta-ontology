###  http://www.semanticweb.org/albafernandez/ontologies/2018/10/untitled-ontology-120#Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :belongsTo ;
                         owl:someValuesFrom :VirtualNode
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :estimates ;
                         owl:someValuesFrom :GridInestabilityEstimation
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasEnergyMarket ;
                         owl:allValuesFrom :EnergyMarket
                       ] ;
       owl:disjointWith :VirtualNode ;
       rdfs:label "Agent" .


###  http://www.semanticweb.org/albafernandez/ontologies/2018/10/untitled-ontology-120#Bid
:Bid rdf:type owl:Class ;
     owl:disjointWith :GridInestabilityEstimation ;
     rdfs:label "Bid" .


###  http://www.semanticweb.org/albafernandez/ontologies/2018/10/untitled-ontology-120#BuyPrice
:BuyPrice rdf:type owl:Class ;
          rdfs:subClassOf <https://w3id.org/saref#Price> ;
          rdfs:label "Buy price" .


###  http://www.semanticweb.org/albafernandez/ontologies/2018/10/untitled-ontology-120#CorrectedEnergy
:CorrectedEnergy rdf:type owl:Class ;
                 rdfs:subClassOf :Property ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasIncentive ;
                                   owl:allValuesFrom :Incentive
                                 ] ;
                 rdfs:label "Corrected energy" .


###  http://www.semanticweb.org/albafernandez/ontologies/2018/10/untitled-ontology-120#EnergyMarket
:EnergyMarket rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :hasPrice ;
                                owl:allValuesFrom <https://w3id.org/saref#Price>
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasTimestamp ;
                                owl:allValuesFrom xsd:dateTimeStamp
                              ] ;
              rdfs:label "Energy market" .


###  http://www.semanticweb.org/albafernandez/ontologies/2018/10/untitled-ontology-120#Flexibility
:Flexibility rdf:type owl:Class ;
             rdfs:subClassOf :Property ;
             rdfs:label "Flexibility" .


###  http://www.semanticweb.org/albafernandez/ontologies/2018/10/untitled-ontology-120#FlexibilityBid
:FlexibilityBid rdf:type owl:Class ;
                rdfs:subClassOf :Bid ,
                                [ rdf:type owl:Restriction ;
